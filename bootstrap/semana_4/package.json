{
  "name": "semana_4",
  "version": "1.0.0",
  "description": "Semana 4",
  "main": "index.html",
  "scripts": {
    "start": "concurrently \"npm run watch:scss\" \"npm run dev\"",
    "dev": "lite-server",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "clean": "rimraf dist",
    "imagemin": "imagemin img/* --out-dir=dist/img",
    "usemin":"usemin index.html -d dist --htmlmin -o dist/index.html && usemin acerca.html -d dist --htmlmin -o dist/acerca.html && usemin contacto.html -d dist --htmlmin -o dist/contacto.html && usemin productos.html -d dist --htmlmin -o dist/productos.html"
  },
  "git": {},
  "author": "Dino Tomassini",
  "license": "ISC",
  "devDependencies": {
    "cssmin": "^0.4.3",
    "htmlmin": "0.0.7",
    "imagemin": "^7.0.1",
    "imagemin-cli": "^6.0.0",
    "less": "^3.12.2",
    "lite-server": "^2.5.4",
    "node-sass": "^4.14.1",
    "onchange": "^7.0.2",
    "rimraf": "^3.0.2",
    "uglifyjs": "^2.4.11",
    "usemin": "^0.6.0"
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "concurrently": "^5.3.0",
    "jpegtran-bin": "^5.0.2",
    "jquery": "^3.5.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1"
  }
}
