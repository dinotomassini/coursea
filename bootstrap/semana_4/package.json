{
  "name": "semana_4",
  "version": "1.0.0",
  "description": "Semana 4",
  "main": "index.html",
  "scripts": {
    "start": "concurrently \"npm run watch:scss\" \"npm run dev\"",
    "dev": "lite-server",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "scss": "node-sass -o css/ css/",
    "clean": "rimraf dist",
    "imagemin": "imagemin img/* --out-dir=dist/img",
    "usemin": "node_modules/usemin-cli/bin/usemin index.html -d dist --htmlmin -o dist/index.html && node_modules/usemin-cli/bin/usemin acerca.html -d dist --htmlmin -o dist/acerca.html && node_modules/usemin-cli/bin/usemin contacto.html -d dist --htmlmin -o dist/contacto.html && node_modules/usemin-cli/bin/usemin productos.html -d dist --htmlmin -o dist/productos.html",
    "build": "npm run clean && npm run scss && npm run imagemin && npm run usemin"
  },
  "git": {},
  "author": "Dino Tomassini",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.26.12",
    "cssmin": "^0.4.3",
    "del": "^5.1.0",
    "grunt": "^1.5.2",
    "grunt-browser-sync": "^2.2.0",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^3.0.0",
    "grunt-contrib-imagemin": "^4.0.0",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-uglify": "^5.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-filerev": "^2.3.1",
    "grunt-usemin": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-flatmap": "^1.0.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-uglify": "^3.0.2",
    "gulp-usemin": "^0.3.30",
    "htmlmin": "0.0.7",
    "imagemin": "^7.0.1",
    "imagemin-cli": "^6.0.0",
    "jit-grunt": "^0.10.0",
    "less": "^3.12.2",
    "lite-server": "^2.5.4",
    "node-sass": "^4.14.1",
    "onchange": "^7.0.2",
    "rimraf": "^3.0.2",
    "time-grunt": "^2.0.0",
    "uglifyjs": "^2.4.11",
    "usemin": "^0.6.0",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "concurrently": "^5.3.0",
    "jpegtran-bin": "^5.0.2",
    "jquery": "^3.5.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1"
  }
}
